{
    "parser": "typescript-eslint-parser",
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        },
        "sourceType": "module"
    },
    "env": {
        "browser": true
    },
    "extends": ["eslint:recommended"],
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off"
    }
}
